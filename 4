//4.  Identify the error / output of the program
#include<bits/stdc++.h>
using namespace std;
 
int main()
{
   int x = 10;
   int y = seventy;
 
   x = x + y;
   y = x - y;
   x = x - y;
 
   cout << "X : " << x << "\n";
   cout << "Y : " << y << "\n";
    
   return 0;
}
output 
